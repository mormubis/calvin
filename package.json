  "scripts": {
    "build": "rollup -c",
    "format": "npm run format:ci -- --write",
    "format:ci": "prettier --ignore-path .gitignore -l \"**/*.+(js|json|md|yml)\"",
    "lint": "eslint \"**/*.js\" --fix",
    "lint:ci": "npm run lint -- --max-warnings 0",
    "postversion": "gitmoji --init",
